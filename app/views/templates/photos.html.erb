<div class="modal fade" id="photosModal" tabindex="-1" role="dialog">
  <div class="modal-dialog modal-lg" role="document">
    <div class="modal-content">
      <div class="modal-body">
        <div class="row close-button text-right">
          <span class="fa fa-times" data-dismiss="modal"></span>
        </div>
        <div photos-carousel class="owl-carousel owl-theme">
          <div ng-repeat="photo in photos">
            <div ng-bind-html="photo.image_tag"></div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
